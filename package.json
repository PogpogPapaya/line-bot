{
  "name": "line-bot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev src",
    "build": "tsc --build",
    "start": "node ./build/index.js",
    "proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./proto --grpc_out=./proto -I ./proto ./proto/*.proto",
    "proto-type": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./proto -I ./proto ./proto/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "@grpc/proto-loader": "^0.6.4",
    "@line/bot-sdk": "^7.3.0",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "google-protobuf": "^3.18.0-rc.2",
    "grpc": "^1.24.11",
    "nanoid": "^3.1.25",
    "sharp": "^0.29.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.6",
    "@types/sharp": "^0.28.5",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  }
}
